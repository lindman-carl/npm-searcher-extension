{
  "name": "NPM Searcher",
  "description": "Search for npm packages in your Chrome omnibox",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "omnibox": { "keyword": "npm" },
  "permissions": ["tabs"],
  "icons": {
    "16": "npm-icon-16.png",
    "48": "npm-icon-48.png",
    "128": "npm-icon-128.png"
  }
}
